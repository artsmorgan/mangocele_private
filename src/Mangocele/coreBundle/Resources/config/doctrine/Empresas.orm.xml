<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Mangocele\coreBundle\Entity\Empresas" table="empresas">
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idCliente" type="integer" column="id_cliente" nullable="false"/>
    <field name="nombre" type="string" column="nombre" length="45" nullable="true"/>
    <field name="descripcion" type="string" column="descripcion" length="255" nullable="true"/>
    <field name="telefonoOficina" type="string" column="telefono_oficina" length="45" nullable="true"/>
    <field name="telefonoEmpresa" type="string" column="telefono_empresa" length="45" nullable="true"/>
    <field name="fax" type="string" column="fax" length="45" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="direccion" type="string" column="direccion" length="45" nullable="true"/>
    <field name="cordX" type="float" column="cord_x" precision="10" scale="0" nullable="true"/>
    <field name="cordY" type="float" column="cord_y" precision="10" scale="0" nullable="true"/>
    <field name="facebook" type="string" column="facebook" length="255" nullable="true"/>
    <field name="twitter" type="string" column="twitter" length="255" nullable="true"/>
    <field name="linkedin" type="string" column="linkedin" length="255" nullable="true"/>
    <field name="youtube" type="string" column="youtube" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
